 public class Employee {
    private int id;
    private String name;
    private int age;
    private double salary;

    private int getId() {
        return this.id;
    }

    private void setId(int id) {
        this.id = id;
    }

    private String getName() {
        return this.name;
    }

    private void setName(String name) {
        this.name = name;
    }

    private int getAge() {
        return this.age;
    }

    private void setAge(int age) {
        this.age = age;
    }

    private double getSalary() {
        return this.salary;
    }

    private void setSalary(double salary) {
        this.salary = salary;
    }
}

# Part A
HTTP GET http://localhost:8080/employees

# Part B
HTTP GET http://localhost:8080/employees/{1}

# Part C
HTTP POST http://localhost:8080/employees/

# Part D
HTTP PUT http://localhost:8080/employees/

# Part E
HTTP DELETE http://localhost:8080/employees/
